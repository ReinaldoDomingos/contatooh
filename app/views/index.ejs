<!DOCTYPE html>
<html lang="pt-br" ng-app="contatooh">
<head>
    <title>Contatooh</title>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <!-- build:css css/index.min.css -->
    <link href="vendor/bootstrap/dist/css/bootstrap.css" rel="stylesheet" type="text/css"/>
    <!-- endbuild -->
</head>
<body class="container">
<% if (usuarioLogado.length > 0) { %>
    <div class="text-right">
        <p>Logado como: <span id="usuario-logado"><%= usuarioLogado %></span>
            <a href="/logout">Logout</a></p>
    </div>
<% } %>
<div ng-view></div>

<!-- build:js js/index.min.js -->
<script src="vendor/angular/angular.js" type="text/javascript"></script>
<script src="vendor/angular-route/angular-route.js" type="text/javascript"></script>
<script src="vendor/angular-resource/angular-resource.js" type="text/javascript"></script>
<script src="js/main.js" type="text/javascript"></script>
<script src="js/services/ContatoService.js" type="text/javascript"></script>
<script src="js/services/InterceptorService.js"></script>
<script src="js/controllers/ContatosController.js" type="text/javascript"></script>
<script src="js/controllers/ContatoController.js" type="text/javascript"></script>
<!-- endbuild -->
</body>
</html>
